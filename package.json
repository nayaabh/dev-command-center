{
  "name": "dev-command-center",
  "version": "1.0.0",
  "description": "",
  "main": "dist/web/index.js",
  "scripts": {
    "start": "npm run dev",
    "build": "webpack --env production",
    "build-e": "webpack --config webpack.electron.config.js --env production",
    "dev": "webpack-dev-server --env development --open --hot",
    "start-e": "npm run build-e & electron ./dist/application/index.js",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Shopping List\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "Abhilash",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-react": "^7.0.0-beta.54",
    "@babel/preset-stage-0": "^7.0.0-beta.54",
    "babel-loader": "^8.0.0-beta.4",
    "classnames": "^2.2.6",
    "css-loader": "^0.28.11",
    "electron": "^2.0.5",
    "electron-connect": "^0.6.3",
    "electron-packager": "^12.1.0",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-icons": "^2.2.7",
    "socket.io-client": "^2.1.1",
    "style-loader": "^0.20.3",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.2"
  },
  "dependencies": {}
}
